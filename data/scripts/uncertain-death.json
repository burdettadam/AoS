  "modifiers": [
    {
      "type": "mutuallyExclusive",
      "characters": [
        "soldier",
        "saint"
      ],
      "note": "Soldier and Saint provide overlapping survival abilities."
    },
    {
      "type": "mutuallyExclusive",
      "characters": [
        "mayor",
        "saint"
      ],
      "note": "Mayor and Saint both provide execution protection."
    },
    {
      "type": "requires",
      "whenCharacter": "drunk",
      "requireCharacters": [
        "chef",
        "empath",
        "fortune-teller",
        "investigator",
        "librarian",
        "washerwoman"
      ],
      "note": "Drunk needs Townsfolk with abilities to malfunction."
    },
    {
      "type": "requires",
      "whenCharacter": "recluse",
      "requireCharacters": [
        "undertaker",
        "empath",
        "fortune-teller"
      ],
      "note": "Recluse benefits from characters that can detect or confirm alignment changes."
    },
    {
      "type": "adjustCounts",
      "whenCharacter": "baron",
      "delta": {
        "outsiders": 2,
        "townsfolk": -2
      },
      "note": "Baron adds 2 Outsiders and removes 2 Townsfolk."
    },
    {
      "type": "adjustCounts",
      "whenCharacter": "godfather",
      "delta": {
        "outsiders": 1,
        "townsfolk": -1
      },
      "note": "Godfather adds 1 Outsider and removes 1 Townsfolk."
    }
  ]
